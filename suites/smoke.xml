<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Sauce Demo Smoke Suite" allow-return-values="true">
  <listeners>
    <listener class-name="org.uncommons.reportng.HTMLReporter"/>
    <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
  </listeners>

  <test name="Cart Test Cases">
    <classes>
      <class name="org.training.selenium.tests.CartTest"/>
    </classes>
  </test>

  <test name="Login Test Cases">
    <classes>
      <class name="org.training.selenium.tests.LoginTest"/>
    </classes>
  </test>
</suite>